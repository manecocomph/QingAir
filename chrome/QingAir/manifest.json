{
  "manifest_version": 2,

  "name": "重点城市空气质量",
  "description": "显示中国重点城市空气质量实时信息.",
  "version": "0.1",

  "permissions": [
    "http://jindu.sinaapp.com/"
  ],

  "icons": { 
	"16": "qIcon16.png",
         "48": "qIcon48.png",
         "128": "qIcon128.png"
   },

  "browser_action": {
    "default_title": "空气质量",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
